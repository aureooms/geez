{
  "name": "geez",
  "description": "Quickly apply git commands to all your repos",
  "version": "2.0.5",
  "author": "Aur√©lien Ooms <aurelien.ooms@gmail.com>",
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
