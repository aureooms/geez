        "sourceMaps": "inline"
      }
    }
  },
  "bin": "lib/cli.js",
  "bugs": {
    "url": "https://github.com/aureooms/geez/issues"
  },
  "dependencies": {
    "@aureooms/listr-aggregate-renderer": "0.0.4",
