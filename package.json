  "devDependencies": {
    "@babel/cli": "7.10.5",
    "@babel/core": "7.11.1",
    "@babel/preset-env": "7.11.0",
    "np": "5.2.1"
  },
  "files": [
    "lib"
  ],
  "homepage": "https://github.com/aureooms/geez#readme",
  "license": "AGPL-3.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aureooms/geez.git"
  },
  "scripts": {
    "build": "rm -rf lib && babel src -d lib",
